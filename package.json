{
  "name": "entask",
  "version": "1.5.0",
  "description": "",
  "author": {
    "name": "streamich",
    "url": "https://github.com/streamich"
  },
  "homepage": "https://github.com/streamich/entask",
  "repository": "streamich/entask",
  "license": "Unlicense",
  "engines": {
    "node": ">=6.9"
  },
  "main": "index.js",
  "browser": "browser.js",
  "files": [
    "index.js",
    "index.d.ts",
    "lite.js",
    "lite.d.ts",
    "browser.js",
    "browser.d.ts"
  ],
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.{ts,tsx,js,jsx}'",
    "prettier:diff": "prettier -l 'src/**/*.{ts,tsx,js,jsx}'",
    "prepush": "yarn prettier:diff",
    "precommit": "pretty-quick --staged && yarn eslint",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "clean": "rimraf lib",
    "build": "echo 'already built'",
    "eslint": "eslint '{index,lite}.js'",
    "test": "jest --no-cache --config='jest.config.js'",
    "release": "semantic-release"
  },
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.1",
    "@types/jest": "^23.3.10",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.12.4",
    "eslint": "^5.10.0"
  },
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "release": {
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
